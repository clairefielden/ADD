{
    "target_preparation": {
        "header": {
            "logging": {
                "logfile": "./AutoDock_Vina_demo/ADV_target_prep.log"
            }
        },
        "input_path": "./DockStreamCommunity/data/1UYD/1UYD_apo.pdb",
        "fixer": {
            "enabled": true,
            "standardize": true,
            "remove_heterogens": true,
            "fix_missing_heavy_atoms": true,
            "fix_missing_hydrogens": true,
            "fix_missing_loops": false,
            "add_water_box": false,
            "fixed_pdb_path": "./AutoDock_Vina_demo/ADV_fixed_target.pdb"
        },
        "runs": [
            {
                "backend": "AutoDockVina",
                "output": {
                    "receptor_path": "./AutoDock_Vina_demo/ADV_receptor.pdbqt"
                },
                "parameters": {
                    "pH": 7.4,
                    "extract_box": {
                        "reference_ligand_path": "./DockStreamCommunity/data/1UYD/PU8.pdb",
                        "reference_ligand_format": "PDB"
                    }
                }
            }
        ]
    }
}